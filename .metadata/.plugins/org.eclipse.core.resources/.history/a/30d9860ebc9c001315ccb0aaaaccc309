package com.tonygwu.tutoring.primenumbers;

import acm.program.ConsoleProgram;

public class PrimeNumbers extends ConsoleProgram {
	public void run() {

		int x = readInt("Please enter your number here: ");
		if (x == 2) {
			println("Prime");
		} else {
			int n = 2;
			while (n < x) {
				if (x % n == 0) {
					println("Not prime");
					n = x + 1;
				} else {
					n = n + 1;
					if (n == x) {
						println("Prime");
					}
				}
			}
		}

	}
}
