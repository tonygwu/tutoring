package com.tonygwu.tutoring.primenumbers;

import acm.program.ConsoleProgram;

public class ListPrimeNumbers extends ConsoleProgram {
	
	public void listPrimeNumbers(int n) {
		for (int i = 2; i <= n; i++) {
			for (int j = 2; j < i; j++) {
				if (i % n == 0) {
					break;
				}
			}
			println(i);
		}
	}
}
